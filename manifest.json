{
	"name": "グーグル画像DL",
	"manifest_version": 3,
	"version": "1.0",
	"permissions": [
        "tabs"
    ],
	"action": {
		"default_popup": "popup.html"
	},
    "content_scripts": [
        {
            "matches": [
                "https://www.google.com/search?*sclient=img*",
                "https://www.google.com/search?*tbm=isch*"
            ],
            "js": ["/js/content.js"]
        }
    ]
}